; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Character",
"language": "GDScript",
"path": "res://game/character/Character.gd"
}, {
"base": "Node",
"class": "Combination",
"language": "GDScript",
"path": "res://game/dungeon/Combination.gd"
}, {
"base": "Control",
"class": "Floor",
"language": "GDScript",
"path": "res://game/map/Floor.gd"
}, {
"base": "Reference",
"class": "ReagentDB",
"language": "GDScript",
"path": "res://database/ReagentDB.gd"
}, {
"base": "Resource",
"class": "Recipe",
"language": "GDScript",
"path": "res://database/Recipe.gd"
}, {
"base": "TextureButton",
"class": "Room",
"language": "GDScript",
"path": "res://game/map/Room.gd"
} ]
_global_script_class_icons={
"Character": "",
"Combination": "",
"Floor": "",
"ReagentDB": "",
"Recipe": "",
"Room": ""
}

[application]

config/name="Alchemy"
config/icon="res://assets/images/slime-icon.png"

[autoload]

EnemyManager="*res://game/enemies/EnemyManager.gd"
ReagentManager="*res://game/reagents/ReagentManager.gd"
RecipeManager="*res://database/RecipeManager.gd"

[display]

window/size/width=1360
window/size/height=768

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
